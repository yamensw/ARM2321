generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = env("DATABASE_PROVIDER")
  url      = env("DATABASE_URL")
}

model Listing {
  id          String   @id @default(cuid())
  title       String
  description String
  price       Decimal
  imageUrl    String?
  createdAt   DateTime @default(now())
}
